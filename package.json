{
  "private": true,
  "name": "dodging-fish",
  "version": "0.0.0",
  "license": "MIT",
  "description": "You need to avoid obstacles, keeping the little fish alive.",
  "author": "Nikita Yudin <nikitayudin782@gmail.com>",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=docs^... && changeset publish"
  },
  "homepage": "https://github.com/nblackninja/dodging-fish#readme",
  "bugs": {
    "url": "https://github.com/nblackninja/dodging-fish/issues",
    "email": "nikitayudin782@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nblackninja/dodging-fish.git"
  },
  "keywords": [
    "js",
    "ts",
    "javascript",
    "typescript",
    "game",
    "phaser"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=12.13.0 <18.0.0",
    "npm": ">=8.2.0",
    "yarn": ">=1.22.x"
  },
  "packageManager": "yarn@1.22.15",
  "devDependencies": {
    "@changesets/cli": "^2.24.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "turbo": "^1.3.4",
    "typescript": "^4.7.4"
  }
}
